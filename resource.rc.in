@ICON_RESOURCE_INFO@

1 VERSIONINFO
 FILEVERSION @FILE_VERSION_MAJOR@,@FILE_VERSION_MINOR@,@FILE_VERSION_PATCH@,0
 PRODUCTVERSION @FILE_VERSION_MAJOR@,@FILE_VERSION_MINOR@,@FILE_VERSION_PATCH@,0
 FILEFLAGSMASK 0x17L
#ifdef _DEBUG
 FILEFLAGS 0x1L
#else
 FILEFLAGS 0x0L
#endif
 FILEOS 0x4L
 FILETYPE 0x1L
 FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904b0"
        BEGIN
            VALUE "CompanyName", "Barco N.V."
            VALUE "FileDescription", "@FILE_DESCRIPTION@"
            VALUE "FileVersion", "@FILE_VERSION_MAJOR@.@FILE_VERSION_MINOR@.@FILE_VERSION_PATCH@.0"
            VALUE "InternalName", "@FILE_ORIGINAL_NAME@"
            VALUE "LegalCopyright", "Barco, @FILE_YEAR_RELEASE@"
            VALUE "OriginalFilename", "@FILE_ORIGINAL_NAME@"
            VALUE "ProductName", "@FILE_PRODUCT_NAME@"
            VALUE "ProductVersion", "@FILE_VERSION_MAJOR@.@FILE_VERSION_MINOR@.@FILE_VERSION_PATCH@.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1200
    END
END
